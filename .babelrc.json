{
  "presets": [
    [
      "@babel/preset-env",
      {
        "targets": { "node": "current" }
      }
    ],
    "@babel/preset-react",
    "@babel/preset-typescript",
    [
      "babel-preset-gatsby",
      {
        "reactRuntime": "automatic"
      }
    ]
  ],
  "plugins": ["@babel/plugin-transform-modules-commonjs"]
}
